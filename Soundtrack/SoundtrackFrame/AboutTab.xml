                </FontString>
                <FontString name="SoundtrackFrame_FontStringCredits11" inherits="GameFontNormal"
                            text="Dozer1170/Gart - Dragonflight updates">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringCredits10"
                                relativePoint="BOTTOMLEFT"/>
                    </Anchors>
                </FontString>

                <!-- SLASH COMMANDS -->
                <FontString name="SoundtrackFrame_FontStringSlash1" inherits="GameFontNormal"
                            text="Slash commands: ">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringAbout1" relativePoint="TOPLEFT">
                            <Offset>
                                <AbsDimension x="375" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="SoundtrackFrame_FontStringSlash2" inherits="GameFontNormal"
                            text="   /soundtrack, /st   - Open Soundtrack window">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringSlash1"
                                relativePoint="BOTTOMLEFT">
                            <Offset>
                                <AbsDimension x="10" y="-10"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="SoundtrackFrame_FontStringSlash3" inherits="GameFontNormal"
                            text="   /soundtrack debug  - Turn on/off debug mode">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringSlash2"
                                relativePoint="BOTTOMLEFT"/>
                    </Anchors>
                </FontString>
                <FontString name="SoundtrackFrame_FontStringSlash4" inherits="GameFontNormal"
                            text="   /soundtrack report - Open report frame">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringSlash3"
                                relativePoint="BOTTOMLEFT"/>
                    </Anchors>
                </FontString>

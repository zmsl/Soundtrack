<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Frame name="SoundtrackFrameAboutFrameTemplate" hidden="true" virtual="true">
        <Size>
            <RelDimension x="0.5" y="0.8"/>
        </Size>
        <Anchors>
            <Anchor point="TOP" relativeTo="SoundtrackFrame" relativePoint="TOP"/>
        </Anchors>
        <Layers>
            <Layer level="BACKGROUND">
                <!-- ABOUT -->
                <FontString name="SoundtrackFrame_FontStringAbout1" inherits="GameFontNormal" text="About Soundtrack:">
                    <Anchors>
                        <Anchor point="TOPLEFT">
                            <Offset>
                                <AbsDimension x="50" y="-50"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="SoundtrackFrame_FontStringAbout2" inherits="GameFontNormal"
                            text="Created by: Morricone ">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringAbout1"
                                relativePoint="BOTTOMLEFT">
                            <Offset>
                                <AbsDimension x="10" y="-10"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="SoundtrackFrame_FontStringAbout3" inherits="GameFontNormal"
                            text="Maintained by: Dozer1170/Gartetasa">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringAbout2"
                                relativePoint="BOTTOMLEFT"/>
                    </Anchors>
                </FontString>
                <FontString name="SoundtrackFrame_FontStringAbout3a" inherits="GameFontNormal" text="Version: 3.2.0_era">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringAbout3"
                                relativePoint="BOTTOMLEFT"/>
                    </Anchors>
                </FontString>

                <!-- CREDITS -->
                <FontString name="SoundtrackFrame_FontStringCredits1" inherits="GameFontNormal" text="Credits:">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringAbout1"
                                relativePoint="BOTTOMLEFT">
                            <Offset>
                                <AbsDimension x="0" y="-60"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="SoundtrackFrame_FontStringCredits2" inherits="GameFontNormal"
                            text="Athame - GetGroupEnemyLevel fix (before 1.21)">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringCredits1"
                                relativePoint="BOTTOMLEFT">
                            <Offset>
                                <AbsDimension x="10" y="-10"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>

                <FontString name="SoundtrackFrame_FontStringCredits3" inherits="GameFontNormal"
                            text="csciguy - v1.28 update for 4.0.3 ">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringCredits2"
                                relativePoint="BOTTOMLEFT"/>
                    </Anchors>
                </FontString>
                <FontString name="SoundtrackFrame_FontStringCredits4" inherits="GameFontNormal"
                            text="Darken5 - Music stop fix (1.24c)">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringCredits3"
                                relativePoint="BOTTOMLEFT"/>
                    </Anchors>
                </FontString>
                <FontString name="SoundtrackFrame_FontStringCredits5" inherits="GameFontNormal"
                            text="FluffyBearLina - v1.21 fan update ">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringCredits4"
                                relativePoint="BOTTOMLEFT"/>
                    </Anchors>
                </FontString>
                <FontString name="SoundtrackFrame_FontStringCredits5a" inherits="GameFontNormal"
                            text="GoTest2 - v2.9.0 Battle and Zone updates for 9.0.1">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringCredits5"
                                relativePoint="BOTTOMLEFT"/>
                    </Anchors>
                </FontString>
                <FontString name="SoundtrackFrame_FontStringCredits6" inherits="GameFontNormal"
                            text="IAmParadox - GenerateMyLibrary.py fix (1.17)">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="SoundtrackFrame_FontStringCredits5a"
                                relativePoint="BOTTOMLEFT"/>
